<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.getyourgame" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/app_id" />

        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name=".CarregaCadastros"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".Cadastro"
            android:label="@string/title_activity_cadastro" >
        </activity>
        <activity
            android:name=".Principal"
            android:label="@string/title_activity_principal"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" >

        </activity>
        <activity
            android:name=".PreferenciaUsuario"
            android:label="@string/title_activity_preferencia_usuario" >
        </activity>
        <activity
            android:name=".ListaUsuarioJogo"
            android:label="@string/title_activity_lista_usuario_jogo" >
        </activity>
        <activity
            android:name=".ListaUsuario"
            android:label="@string/title_activity_lista_usuario" >
        </activity>
        <activity
            android:name=".ListaJogo"
            android:label="@string/title_activity_lista_jogo" >
        </activity>
        <activity android:name=".testePicture" >
        </activity>
        <activity
            android:name=".Interesse"
            android:label="@string/title_activity_interesse" >
        </activity>
        <activity
            android:name=".InteresseTab"
            android:label="@string/title_activity_interesse_tab" >
        </activity>
        <activity android:name=".util.CustomGalleryActivity" >
            <intent-filter>
                <action android:name="luminous.ACTION_PICK" />
                <action android:name="luminous.ACTION_MULTIPLE_PICK" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".testeMultipleImages"
            android:label="@string/title_activity_teste_multiple_images"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" >
        </activity>
        <activity
            android:name=".Oportunidades"
            android:label="@string/title_activity_oportunidades"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".IniciarTransacao"
            android:label="@string/title_activity_iniciar_transacao"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".Contatos"
            android:label="@string/title_activity_contatos" >
        </activity>
        <activity
            android:name=".ListaTransacao"
            android:label="@string/title_activity_lista_transacao" >
        </activity>
        <activity
            android:name=".ListaTransacaoTab"
            android:label="@string/title_activity_lista_transacao_tab" >
        </activity>
        <activity
            android:name=".Transacao"
            android:label="@string/title_activity_transacao" >
        </activity>
    </application>

</manifest>
